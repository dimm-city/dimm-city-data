{
    "appService.zipIgnorePattern": [
        ".vscode{,/**}",
        ".docker{,/**}",
        ".git{,/**}",
        ".github{,/**}",
        "tools{,/**}",
        "*ignore*",
        ".editorconfig",
        ".es*",
        ".env.*",
        ".env",
        "*eslint*",
        ".strapi-updater.json",
        "migrate.sh",
        "README.md",
        ".cache",
        "build",
        "node_modules"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/.cache": true,
        "**/build":true,
        "**/.tmp": true
    },
    "cSpell.words": [
        "Dimm",
        "Sporos",
        "strapi"
    ],

}
