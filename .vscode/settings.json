{
    "appService.zipIgnorePattern": [
        "node_modules{,/**}",
        ".vscode{,/**}",
        "Dockerfile",
        ".env.example",
        "eslint*"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules/@strapi": false,
        "**/node_modules": false,
        "**/.cache": true,
        "**/.tmp": true,
        "**/build": true
    },
    "cSpell.words": [
        "Sporos",
        "strapi"
    ]
}