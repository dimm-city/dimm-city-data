{
    "appService.zipIgnorePattern": [
        ".vscode{,/**}",
        ".docker{,/**}",
        ".git{,/**}",
        ".github{,/**}",
        "build{,/**}",
        "tools{,/**}",
        "Dockerfile",
        ".env.*",
        ".env",
        "eslint*",
        ".strapi-updater.json",
        "install_startup.sh",
        "migrate.sh",
        "README.md"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules/@strapi": false,
        "**/node_modules": false,
        "**/.cache": true,
        "**/.tmp": true
    },
    "cSpell.words": [
        "Dimm",
        "Sporos",
        "strapi"
    ],

}
