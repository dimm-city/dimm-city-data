{
  "kind": "collectionType",
  "collectionName": "characters",
  "info": {
    "singularName": "character",
    "pluralName": "characters",
    "displayName": "Character",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
