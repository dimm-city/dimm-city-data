{
  "kind": "collectionType",
  "collectionName": "characters",
  "info": {
    "singularName": "character",
    "pluralName": "characters",
    "displayName": "Character",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "unique": false,
      "required": true
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "age": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "max": 999,
      "min": 1
    },
    "height": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "noticeableTraits": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "backstory": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "clothing": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "movementDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "originLocation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "beliefs": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "flaws": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "dreams": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "race": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::race.race"
    },
    "faction": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::faction.faction",
      "inversedBy": "members"
    },
    "playerCharacter": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "selectedAbilities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ability.ability"
    },
    "historicalEvents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::historical-event.historical-event",
      "inversedBy": "characters"
    },
    "selectedSkillTrees": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill-tree.skill-tree"
    },
    "currentLocation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "tokenId": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid"
    },
    "characterRoles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::character-role.character-role"
    },
    "release": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::character-release.character-release",
      "inversedBy": "characters"
    }
