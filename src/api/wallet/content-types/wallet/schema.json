{
  "kind": "collectionType",
  "collectionName": "wallets",
  "info": {
    "singularName": "wallet",
    "pluralName": "wallets",
    "displayName": "Wallet",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "blockchain": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blockchain.blockchain"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "journalEntries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::journal-entry.journal-entry",
      "mappedBy": "author"
    },
    "seed": {
      "type": "text",
      "private": true,
      "unique": false
    },
    "managed": {
      "type": "boolean",
      "private": true,
      "default": true
    },
    "key": {
      "type": "string",
      "private": true,
      "unique": false
    },
    "encKey": {
      "type": "password"
    },
    "characters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::character.character",
      "mappedBy": "wallet"
    },
    "tokens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::token.token",
      "mappedBy": "wallet"
    }
  }
}
